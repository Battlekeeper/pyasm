from std_lib import *


# Compute factorial of 10
def main():
    index: float = 1.0
    total: float = 1.0

    while index <= 10.0:
        total: float = total * index
        print_float(total)
        print_newline()
        index: float = 1.0 + index
    print_newline()
    print_newline()
    print_float(total)